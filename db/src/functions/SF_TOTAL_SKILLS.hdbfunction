FUNCTION "HANA_TRAINING.functions::SF_TOTAL_SKILLS"(  IN firstname NVARCHAR(50))

RETURNS LV_COUNT INTEGER

LANGUAGE SQLSCRIPT AS

BEGIN

SELECT COUNT(t1.SKILLID) INTO LV_COUNT
FROM "HANA_TRAINING.data::SKILLSET" t1
INNER JOIN "HANA_TRAINING.data::EMPLOYEE" t2 
ON t1.SAPNO = t2.SAPNO
WHERE t2.FIRSTNAME = :firstname AND t1.DATEEND = '2024-09-05';

END;
 