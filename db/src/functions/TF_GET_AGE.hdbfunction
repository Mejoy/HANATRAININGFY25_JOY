FUNCTION "SC_HANAUPS_DESIGN"."PK_D_Upskilling.functions::TF_GET_GROUP_EMPLOYEE"()

RETURNS TABLE (FIRSTNAME NVARCHAR(50), SKILLID INTEGER)

LANGUAGE SQLSCRIPT AS

BEGIN

RETURN SELECT  T1.FIRSTNAME,T2.SKILLID
FROM "SC_HANAUPS_DESIGN"."PK_D_Upskilling.data::CDS_MyTables.TB_DEL_ROSARIO_EMPLOYEE" T1
INNER JOIN "SC_HANAUPS_DESIGN"."PK_D_Upskilling.data::CDS_MyTables.TB_DEL_ROSARIO_SKILLSET" T2 
ON T1.SAPNO = T2.SAPNO GROUP BY T2.SKILLID,T1.FIRSTNAME ORDER BY T1.FIRSTNAME ASC;

END