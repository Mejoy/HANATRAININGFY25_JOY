FUNCTION "HANA_TRAINING.functions::TF_GET_GROUP_SKILL"()

RETURNS TABLE (FIRSTNAME NVARCHAR(50), SKILLID INTEGER)

LANGUAGE SQLSCRIPT AS

BEGIN

RETURN SELECT  t1.FIRSTNAME,t2.SKILLID
FROM "HANA_TRAINING.data::EMPLOYEE" t1
INNER JOIN "HANA_TRAINING.data::SKILLSET" t2 
ON t1.SAPNO = t2.SAPNO GROUP BY t2.SKILLID,t1.FIRSTNAME ORDER BY t1.FIRSTNAME ASC;

END