FUNCTION "HANA_TRAINING.functions::TF_GET_RIRO_DETAILS"()
RETURNS TABLE (EMPLOYEENAME NVARCHAR(50), ROLLINDATE DATE, ROLLOFFDATE  DATE )

LANGUAGE SQLSCRIPT AS

BEGIN

RETURN SELECT t1.FIRSTNAME || ' ' || t1.LASTNAME AS EMPLOYEENAME, 
t2.ROLLINDATE, t2.ROLLOFFDATE 
FROM "HANA_TRAINING.data::EMPLOYEE" t1 
INNER JOIN "HANA_TRAINING.data::PROJECT" t2 ON
t1.SAPNO = t2.SAPNO
WHERE t2.DELETEFLAG = '0' AND t1.DELETEFLAG = '0';

END; 